<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>{{ title }}</title>
  <link rel="shortcut icon" href="{{ img }}favicon.ico" type="image/x-icon">
  <link rel="icon" href="{{ img }}favicon.ico" type="image/x-icon">
  <!-- PACE js -->
  <link rel="stylesheet" href="{{ css }}pace-theme-minimal.css">
  <script data-pace-options='{ "ajax": false, "restartOnRequestAfter": false }' src="{{ js }}pace.min.js"></script>
  <!-- CSS -->
  <link rel="stylesheet" href="{{ css }}bootstrap.min.css">
  <link rel="stylesheet" href="{{ css }}font-awesome.min.css">
  <link rel="stylesheet" href="{{ css }}animate.min.css">
  <link rel="stylesheet" href="{{ css }}styles.css">
  {% block css %}{% endblock %}
  <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
  <![endif]-->
</head>
<body class="container">
  <!--[if lt IE 8]>
  <p class="browsehappy alert alert-warning alert-dismissable">
  <i class="fa fa-warning"></i>
  <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>{{ lang.browsehappy|raw }}</p>
  <![endif]-->
  <div class="row">
    <div class="col-md-4">
      <div class="page-header">
        <h1>{{ title }} <small>by <a href="http://www.adaptivez.org.mx/gisaa/">GISAA</a></small></h1>
      </div>
    </div>
    <div class="col-md-8">
      {% spaceless %}
        {% include 'navbar.twig' %}
      {% endspaceless %}
    </div>
  </div>
  {% block content %}{% endblock %}
  <!-- JS -->
  <script src="{{ js }}jquery.min.js"></script>
  <script src="{{ js }}jquery.cookie.js"></script>
  <script src="{{ js }}bootstrap.min.js"></script>
  <script src="{{ js }}moment-with-langs.min.js"></script>
  <script src="{{ js }}charts.min.js"></script>
  <script src="{{ js }}app.js"></script>
  <script>
    $(document).ready(function() {
      $('*[data-lang]').click(function(event) {
        $.cookie('{{ langCookie }}', $(this).attr('data-lang'));
      });
    });
  </script>
  {% block js %}{% endblock %}
</body>
</html>
